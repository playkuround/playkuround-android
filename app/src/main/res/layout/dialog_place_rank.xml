<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:background="@drawable/badge_dialog_bg">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintWidth_max="450dp"
    app:layout_constraintHeight_max="900dp">


    <ImageView
        android:id="@+id/dialog_get_badge_x_iv"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/agree_x"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="18dp" />

    <TextView
        android:id="@+id/dialog_get_badge_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_get_badge_rank"
        android:textSize="29dp"
        android:fontFamily="@font/pretendard_semi_bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="115dp"
        android:layout_marginStart="18dp"/>

    <ImageView
        android:id="@+id/dialog_get_badge_school_iv"
        android:layout_width="106dp"
        android:layout_height="106dp"
        android:src="@drawable/dialog_get_badge_school"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_title_tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="28dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="169dp"/>

    <ImageView
        android:id="@+id/dialog_get_badge_circle_iv"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:src="@drawable/dialog_get_badge_circle"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_school_iv"
        app:layout_constraintTop_toTopOf="@id/dialog_get_badge_school_iv"
        android:layout_marginTop="58dp"
        android:layout_marginStart="83dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+ 1,234"
        android:textSize="24dp"
        android:fontFamily="@font/pretendard_medium"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_circle_iv"
        app:layout_constraintTop_toTopOf="@id/dialog_get_badge_circle_iv"
        app:layout_constraintEnd_toEndOf="@id/dialog_get_badge_circle_iv"
        app:layout_constraintBottom_toBottomOf="@id/dialog_get_badge_circle_iv"/>


    <TextView
        android:id="@+id/dialog_get_badge_place_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="산학협력관"
        android:textSize="24dp"
        android:fontFamily="@font/pretendard_regular"
        android:background="@color/black"
        android:textColor="@color/green_main"
        android:padding="2dp"
        android:paddingStart="4dp"
        android:paddingEnd="4dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_title_tv"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_circle_iv"
        android:layout_marginTop="13dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="을"
        android:textSize="24dp"
        android:fontFamily="@font/pretendard_extra_light"
        app:layout_constraintStart_toEndOf="@id/dialog_get_badge_place_tv"
        app:layout_constraintTop_toTopOf="@id/dialog_get_badge_place_tv"
        app:layout_constraintBottom_toBottomOf="@id/dialog_get_badge_place_tv"
        android:layout_marginStart="5dp"/>

    <TextView
        android:id="@+id/dialog_get_badge_visit_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_get_badge_visit"
        android:textSize="24dp"
        android:fontFamily="@font/pretendard_extra_light"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_place_tv"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_place_tv"
        android:layout_marginTop="5dp"/>

    <TextView
        android:id="@+id/dialog_get_badge_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="건덕이"
        android:textSize="24dp"
        android:fontFamily="@font/pretendard_regular"
        android:background="@color/green_main"
        android:textColor="@color/black"
        android:padding="2dp"
        android:paddingStart="4dp"
        android:paddingEnd="4dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_place_tv"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_visit_tv"
        android:layout_marginTop="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="님입니다."
        android:textSize="24dp"
        android:fontFamily="@font/pretendard_extra_light"
        app:layout_constraintStart_toEndOf="@id/dialog_get_badge_name_tv"
        app:layout_constraintTop_toTopOf="@id/dialog_get_badge_name_tv"
        app:layout_constraintBottom_toBottomOf="@id/dialog_get_badge_name_tv"
        android:layout_marginStart="5dp"/>

    <View
        android:id="@+id/dialog_get_badge_tabbar1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_name_tv"
        android:layout_marginTop="38dp"/>

    <LinearLayout
        android:id="@+id/dialog_get_badge_bar"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintTop_toBottomOf="@+id/dialog_get_badge_tabbar1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/ranking_index"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/ranking_nickname"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/ranking_score"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp"/>

    </LinearLayout>

    <View
        android:id="@+id/dialog_get_badge_tabbar2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialog_get_badge_bar"/>

    <LinearLayout
        android:id="@+id/dialog_get_badge_my_bar"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="1"
                android:fontFamily="@font/pretendard_semi_bold"
                android:textSize="16dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/dialog_get_badge_rank_name_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="건덕이"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/dialog_get_rank_score_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="841"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintEnd_toEndOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar"
        android:background="@color/lighter_gray"/>

    <LinearLayout
        android:id="@+id/dialog_get_badge_my_bar2"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="1"
                android:fontFamily="@font/pretendard_semi_bold"
                android:textSize="16dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/dialog_get_badge_rank_name2_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="건덕이"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/dialog_get_rank_score2_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="841"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintEnd_toEndOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar2"
        android:background="@color/lighter_gray"/>

    <LinearLayout
        android:id="@+id/dialog_get_badge_my_bar3"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="1"
                android:fontFamily="@font/pretendard_semi_bold"
                android:textSize="16dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/dialog_get_badge_rank_name3_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="건덕이"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/dialog_get_rank_score3_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="841"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintEnd_toEndOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar3"
        android:background="@color/lighter_gray"/>

    <LinearLayout
        android:id="@+id/dialog_get_badge_my_bar4"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="1"
                android:fontFamily="@font/pretendard_semi_bold"
                android:textSize="16dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/dialog_get_badge_rank_name4_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="건덕이"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/dialog_get_rank_score4_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="841"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintEnd_toEndOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar4"
        android:background="@color/lighter_gray"/>

    <LinearLayout
        android:id="@+id/dialog_get_badge_my_bar5"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="1"
                android:fontFamily="@font/pretendard_semi_bold"
                android:textSize="16dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/dialog_get_badge_rank_name5_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="건덕이"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

        <TextView
            android:id="@+id/dialog_get_rank_score5_tv"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="841"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp" />

    </LinearLayout>



    <LinearLayout
        android:id="@+id/dialog_get_rank_score_my_bar"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal"
        android:background="@color/lighter_gray">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="485"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp"
            android:id="@+id/ranking_my_rank_tv"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/ranking_me"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="72"
            android:fontFamily="@font/pretendard_semi_bold"
            android:textSize="16dp"
            android:id="@+id/ranking_my_score_tv"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintEnd_toEndOf="@id/dialog_get_badge_tabbar2"
        app:layout_constraintTop_toBottomOf="@id/dialog_get_badge_my_bar5"
        android:background="@color/lighter_gray"/>


</androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>